{
    "workspace.rootPatterns": [
        ".vim",
        ".git",
        ".hg",
        ".projections.json"
    ],
    "workspace.bottomUpFiletypes": ["*"],
    "Lua.completion.enable": true,
    "Lua.hint.enable": true,
    "Lua.diagnostics.globals": [
        "CS", "typeof"
    ],
    "codeLens.enable": true,
    "diagnostic.virtualText": false
}
